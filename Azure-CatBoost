     "inputWidgets": {},
     "nuid": "6a70d283-2667-47cf-a011-b608894622bf",
     "showTitle": false,
     "tableResultSettingsMap": {},
     "title": ""
    }
   },
   "outputs": [],
   "source": [
    "df_outlier_removed = remove_outliers_iqr(pandas_df)\n"
   ]
