   "cell_type": "code",
   "execution_count": 0,
   "metadata": {
    "application/vnd.databricks.v1+cell": {
     "cellMetadata": {
      "byteLimit": 2048000,
      "rowLimit": 10000
     },
     "inputWidgets": {},
     "nuid": "33b0ae61-10dd-4ec9-a396-d69d0e16a783",
     "showTitle": false,
     "tableResultSettingsMap": {},
     "title": ""
