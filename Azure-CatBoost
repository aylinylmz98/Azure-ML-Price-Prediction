    "# Hedef ve özellikleri tekrar hatırlatalım:\n",
    "X = df_encoded.drop([\"Price(TRY)_log\"], axis=1)\n",
    "y = df_encoded[\"Price(TRY)_log\"]\n",
    "\n",
    "results = compare_models(X, y)\n",
    "print(results)\n",
    "\n"
