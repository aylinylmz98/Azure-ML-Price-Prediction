    "\n",
    "# 1. Scale edilecek sütunları belirle\n",
    "columns_to_scale = [\n",
    "     \"Mileage(km)\", \"EngineSize(cc)\", \"EnginePower(HP)\",\n",
    "    \"NEW_CarAge\", \"NEW_KmPerYear\", \"NEW_AvgPricePerBrandSeries\",\n",
